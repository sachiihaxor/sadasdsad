RewriteRule ^pengumuman(.*)$ pengumuman.php [L]
RewriteRule ^dpengumuman-(.*)$ dpengumuman.php?url=$1 [L]
RewriteRule ^download-(.*)$ download.php?file=$1 [L]
 
 RewriteRule ^agenda(.*)$ agenda.php [L]
